{
  "name": "my_store",
  "version": "1.0.0",
  "description": "curso de backend con nodejs - platzi",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./src/index.js",
    "start": "node ./src/index.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:delete-last": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli  db:migrate:undo:all ",
    "migrations:up": "npx sequelize-cli db:migrate",
    "migrations:down": "npx sequelize-cli db:migrate:undo",
    "migrations:down:all": "npx sequelize-cli db:migrate:undo:all",
    "seeders:generate":"sequelize-cli seed:generate --name",
    "seeders:run":"sequelize-cli db:seed:all",
    "seeders:delete":"sequelize-cli db:seed:undo:all",
    "dev:dir": "mkdir postgres_data mysql_data",
    "docker:mysql": "docker-compose up -d mysql",
    "docker:phpmyadmin": "docker-compose up -d phpmyadmin",
    "docker:postgres": "docker-compose up -d postgres",
    "docker:pgadmin": "docker-compose up -d pgadmin",
    "docker:down": "docker-compose down",
    "docker:up": "docker-compose up -d",
    "docker:mysql:run":"docker-compose up -d mysql & docker-compose up -d phpmyadmin",
    "docker:postgres:run":"docker-compose up -d postgres & docker-compose up -d pgadmin"
  },
  "keywords": [
    "nodejs",
    "platzi",
    "api",
    "rest",
    "database",
    "crud",
    "javascript"
  ],
  "author": "jhonatan alberto padilla rovira <jhoalparo1991@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "dotenv": "^16.0.3",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "mysql2": "^3.0.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0"
  },
  "devDependencies": {
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2"
  }
}
